{
  "name": "amplify-datastore-rxjs",
  "version": "1.0.9",
  "description": "RxJs Subjects to work with AWS Amplify and Amplify Datastore.",
  "license": "MIT",
  "author": "Tomasz Gorka",
  "homepage": "https://github.com/tgorka/amplify-datastore-rxjs",
  "bugs": {
    "url": "https://github.com/tgorka/amplify-datastore-rxjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tgorka/amplify-datastore-rxjs.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "files": [
    "/dist",
    "README.md",
    "LICENSE",
    "package.json",
    "package-lock.json"
  ],
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "graphql-client",
    "graphql",
    "aws",
    "angular",
    "typescript",
    "rxjs",
    "amplify",
    "datastore",
    "awsamplify",
    "amplifydatastore",
    "angular",
    "typescript",
    "ionic",
    "data",
    "scroll",
    "fetch"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build:noclean": "tsc",
    "build": "npm run clean:dist && npm run build:noclean",
