{
    "name": "amplify-datastore-rxjs",
    "version": "1.0.0",
    "description": "RxJs Subjects to work with AWS Amplify and Amplify Datastore.",
    "license": "MIT",
    "author": "Tomasz Gorka",
    "homepage": "https://github.com/tgorka/amplify-datastore-rxjs",
    "bugs": {
      "url": "https://github.com/tgorka/amplify-datastore-rxjs/issues"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/tgorka/amplify-datastore-rxjs.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
      "/dist",
      "README.md",
      "LICENSE",
      "package.json",
      "package-lock.json"
    ],
    "engines": {
        "node": ">=16"
    },
    "keywords": [
      "graphql-client", 
      "graphql", 
      "aws", 
      "angular", 
      "typescript",
      "rxjs", 
      "amplify", 
      "datastore", 
      "awsamplify", 
      "amplifydatastore",
      "angular",
      "typescript",
      "ionic",
      "data",
      "scroll",
      "fetch"
    ],
    "scripts": {
        "prepare": "npm run build",
        "build:noclean": "tsc",
        "build": "npm run clean && npm run build:noclean",
        "clean": "rm -rf ./dist",
        "publish:nobuild": "npm login && npm publish",
        "publish": "npm run build && npm run publish:nobuild"
    },
    "dependencies": {
        "@aws-amplify/datastore": "^3.5.0",
        "rxjs": "^7.4.0"
    },
    "devDependencies": {
        "typescript": "^4.4.4"
    }
}
